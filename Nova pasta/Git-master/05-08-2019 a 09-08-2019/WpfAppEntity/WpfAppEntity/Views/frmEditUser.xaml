        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEntity.Views"
        mc:Ignorable="d"
        Title="frmEditUser" Height="240" Width="470">
    <Grid Margin="0,0,4.2,8">
        <TextBox Name="txtName" HorizontalAlignment="Left" Height="33" Margin="74,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="359"/>
        <TextBox Name="txtLogin" HorizontalAlignment="Left" Height="33" Margin="74,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="359"/>
        <TextBox Name="txtSenha" HorizontalAlignment="Left" Height="33" Margin="74,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="359"/>
        <Button Click="Button_Click" Content="Salvar" HorizontalAlignment="Left" Height="38" Margin="21,157,0,0" VerticalAlignment="Top" Width="412"/>
        <Label Content="Nome :" HorizontalAlignment="Left" Margin="21,20,0,0" VerticalAlignment="Top"/>
        <Label Content="Login :" HorizontalAlignment="Left" Margin="21,63,0,0" VerticalAlignment="Top"/>
        <Label Content="Senha :" HorizontalAlignment="Left" Margin="21,108,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.586,1.931"/>

    </Grid>
</Window>
